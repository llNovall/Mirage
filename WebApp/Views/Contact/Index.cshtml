@model ContactInputModel 

@{
    if(Model.EmailSendStatus == EmailSendStatus.Pending)
    {
        <partial name="_EmailSendPendingPartial" model="@Model" />

        return;
    }

    if (Model.EmailSendStatus == EmailSendStatus.Success)
    {
        <partial name="_EmailSendSuccessPartial" />

        return;
    }

    if (Model.EmailSendStatus == EmailSendStatus.Failed)
    {
        <partial name="_EmailSendFailedPartial" />

        return;
    }

}
